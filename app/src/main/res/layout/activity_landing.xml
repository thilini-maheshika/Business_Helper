<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity.Landing"
    android:background="@drawable/iphone_14_pro__1">

    <ProgressBar
        android:id="@+id/progressBar"
        style="?android:attr/progressBarStyle"
        android:layout_width="153dp"
        android:layout_height="166dp"
        android:layout_marginTop="60dp"
        android:indeterminateTint="#ADC1D1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/business_name" />

    <TextView
        android:id="@+id/business_name"
        android:layout_width="282dp"
        android:layout_height="173dp"
        android:layout_marginTop="96dp"
        android:backgroundTint="#CFBABA"
        android:text="Business Helper"
        android:textAlignment="center"
        android:textColor="#FFFCFC"
        android:textColorHighlight="#4B95CF"
        android:textColorHint="#EDDEDE"
        android:textSize="48sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>